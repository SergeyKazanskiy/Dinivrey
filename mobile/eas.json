{
  "cli": {
    "version": ">= 13.0.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "ENV_FILE": ".env.development"
      }
    },
    "preview": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENV_FILE": ".env.production"
      }
    },
    "production": {
      "distribution": "store",
      "env": {
        "ENV_FILE": ".env.production"
      }
    }
  }
}
